const getAccountByFoundAccountId = (
  selectedShortTermFunds,
  fundAccountIdRef,
) => {
  let selectedAccount = {}
  _.forEach(selectedShortTermFunds.selected, entity => {
    const account = _.find(
      entity.accounts,
      item => item.id === fundAccountIdRef,
    )
    if (account) {
      selectedAccount = account
    }
  })
  return selectedAccount
}
